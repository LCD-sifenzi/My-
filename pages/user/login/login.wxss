
.view-Logo {
  background-color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 100rpx 0rpx;
}

.view-Wx {
  background-color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 100rpx 0rpx;
  margin-bottom: 0;
}
.image-Wx {
  width: 100rpx;
  height: 100rpx;
}
.image-Logo {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;

}
.image-Icon{
  width: 50rpx;
  height: 50rpx;
}
.view-Item {
  background-color: white;
  height: 120rpx;
  width: 600rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx 0rpx;
}
.input-Name {
  margin-left: 20rpx;
  font-size: 40rpx;
  width: 530rpx;
}
.text-Name {
  color: #353439;
  font-size: 40rpx;
}


.button-Login {
  width: 600rpx;
  margin-top: 50rpx;
  
}

.view-register {
  background-color: white;
  width:600rpx;
  display: flex;
  margin-top: 10rpx;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.button-register {
  background-color: white;
  color: grey;
  border-color: white;
  font-size: 36rpx;
  margin-left: 0;
  margin-right: 0;
}
.button-register::after {
  border-color: white;
}
